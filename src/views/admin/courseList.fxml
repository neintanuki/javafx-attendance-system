<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="396.0" prefWidth="712.0" spacing="16.0" stylesheets="@../list.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="views.admin.CourseList">
   <children>
      <Button fx:id="mainAction" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#openAddCourse" prefHeight="44.0" prefWidth="777.0" styleClass="listBtn" text="Add Course" VBox.vgrow="ALWAYS" />
      <ScrollPane fitToHeight="true" fitToWidth="true" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
         <content>
            <TableView fx:id="courseTable" maxHeight="1.7976931348623157E308" prefHeight="263.0" prefWidth="777.0" tableMenuButtonVisible="true">
               <columns>
                  <TableColumn fx:id="id" prefWidth="81.0" text="ID" />
                  <TableColumn fx:id="courseTitle" prefWidth="155.0" text="Course Title" />
                  <TableColumn fx:id="assignedTeacher" prefWidth="75.0" text="Assigned Teacher" />
                  <TableColumn fx:id="yearStart" prefWidth="75.0" text="Year Start" />
                  <TableColumn fx:id="yearEnd" prefWidth="75.0" text="Year End" />
                  <TableColumn fx:id="actions" prefWidth="75.0" text="Actions" />
               </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </content>
      </ScrollPane>
   </children>
</VBox>
